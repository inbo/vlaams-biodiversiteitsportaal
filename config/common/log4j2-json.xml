<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
             <JSONLayout />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="${LOG_LEVEL:-INFO}">
            <AppenderRef ref="console"/>
        </Root>
    </Loggers>
</Configuration>
