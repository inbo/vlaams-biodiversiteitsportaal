{
    "interactiveLogin": false,
    "httpServer": "NettyWrapper",
    "loginPagePath" : "/config/login.html",
    "tokenCallbacks": [
        {
            "issuerId": "mock-oauth2",
            "tokenExpiry": 120,
            "requestMappings": [
                {
                    "requestParam": "grant_type",
                    "match": "*",
                    "claims": {
                        "client_id": "some-client",
                        "sub": "some-user-id",
                        "something": "else"
                    }
                }
            ]
        }
    ]
}