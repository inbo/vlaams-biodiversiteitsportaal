<?xml version="1.0" encoding="UTF-8"?>
<!-- collectory -->
<Context reloadable="false" useHttpOnly="false">

    <Parameter name="spring.config.additional-location"
               value="file:/config/common/common.yml,file:/config/collectory/collectory.yml,${ADDITIONAL_SPRING_CONFIG_LOCATIONS}"
               override="false"/>
    <Parameter name="grails.env"
               value="${GRAILS_ENV}"
               override="false"/>

    <!-- Logging config -->
    <Parameter name="appName"
               value="%cyan(collectory)"
               override="false"/>
</Context>