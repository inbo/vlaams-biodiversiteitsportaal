<?xml version="1.0" encoding="UTF-8"?>
<!-- species-list -->
<Context reloadable="false" useHttpOnly="false">

    <Parameter name="spring.config.additional-location"
               value="file:/config/common/common.yml,file:/config/species-list/species-list.yml,${ADDITIONAL_SPRING_CONFIG_LOCATIONS}"
               override="false"/>
    <Parameter name="grails.env"
               value="${GRAILS_ENV}"
               override="false"/>

    <!-- Logging config -->
    <Parameter name="appName"
               value="%boldWhite(species-list)"
               override="false"/>
</Context>