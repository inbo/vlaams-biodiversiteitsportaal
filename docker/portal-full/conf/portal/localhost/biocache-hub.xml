<?xml version="1.0" encoding="UTF-8"?>
<!-- biocache-hub -->
<Context reloadable="false" useHttpOnly="false">

    <Parameter name="spring.config.additional-location"
               value="file:/config/common/common.yml,file:/config/biocache-hub/biocache-hub.yml,${ADDITIONAL_SPRING_CONFIG_LOCATIONS}"
               override="false"/>
    <Parameter name="grails.env"
               value="${GRAILS_ENV}"
               override="false"/>

    <!-- Logging config -->
    <Parameter name="appName"
               value="%magenta(biocache-hub)"
               override="false"/>
</Context>