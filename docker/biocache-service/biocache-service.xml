<?xml version="1.0" encoding="UTF-8"?>
<!-- biocache-service -->
<Context reloadable="false">

    <Parameter name="spring.config.additional-location"
               value="file:/config/config/common/common.yml,file:/config/config/biocache-service/biocache-service.yml,${ADDITIONAL_SPRING_CONFIG_LOCATIONS}"
               override="false"/>

    <!-- Logging config -->
    <Parameter name="logging.config"
               value="/config/config/biocache-service/log4j-docker.xml"
               override="false"/>
    <Parameter name="appName"
               value="biocache-service"
               override="false"/>
</Context>