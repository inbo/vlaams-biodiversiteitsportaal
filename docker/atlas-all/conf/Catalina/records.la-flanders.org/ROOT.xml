<?xml version="1.0" encoding="UTF-8"?>
<!-- biocache-hub -->
<Context reloadable="false">

    <Parameter name="spring.config.additional-location"
               value="file:/config/config/common/common.yml,file:/config/config/common/common-docker.yml,file:/config/config/ala-hub/ala-hub.yml"
               override="false"/>

    <!-- Logging config -->
    <Parameter name="logging.config"
               value="/config/config/common/logback-docker.xml"
               override="false"/>
    <Parameter name="appName"
               value="biocache-hub"
               override="false"/>
</Context>