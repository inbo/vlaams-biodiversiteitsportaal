<?xml version="1.0" encoding="UTF-8"?>
<!-- biocache-service -->
<Context reloadable="false">

    <Parameter name="spring.config.additional-location"
               value="file:/config/config/common/common.yml,file:/config/config/biocache-service/biocache-service.yml"
               override="false"/>
    <Parameter name="common.protocol"
               value="http"
               override="false"/>
    <Parameter name="common.domain"
               value="la-flanders.org"
               override="false"/>

    <!-- Logging config -->
    <Parameter name="logging.config"
               value="/config/config/common/logback-docker.xml"
               override="false"/>
    <Parameter name="appName"
               value="biocache-service"
               override="false"/>
</Context>