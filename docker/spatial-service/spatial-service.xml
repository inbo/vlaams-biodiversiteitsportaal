<?xml version="1.0" encoding="UTF-8"?>
<!-- spatial-service -->
<Context reloadable="false">

    <Parameter name="spring.config.additional-location"
               value="file:/config/config/common/common.yml,file:/config/config/spatial-service/spatial-service.yml,${ADDITIONAL_SPRING_CONFIG_LOCATIONS}"
               override="false"/>
